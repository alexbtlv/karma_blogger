<div id="trianglify-register">
  <div class="row">
    <div class="small-12 small-centered medium-8 medium-centered large-6 large-centered columns">
      <h2 class="text-center">Sign up</h2>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>

        <div class="row">
          <div class="small-5 column">
            <%= f.label :name, class: "right inline" %>
          </div>
          <div class="small-7 column">
            <%= f.text_field :name, autofocus: true, id: "right-label" %>
          </div>
        </div>

        <div class="row">
          <div class="small-5 column">
            <%= f.label :email, class: "right inline" %>
          </div>
          <div class="small-7 column">
            <%= f.email_field :email, id: "right-label"  %>
          </div>
        </div>

        <div class="row">
          <div class="small-5 column">
            <%= f.label :password, class: "right inline" %>
          </div>
          <div class="small-7 column">
            <%= f.password_field :password, autocomplete: "off", id: "right-label" %>
          </div>
        </div>

        <div class="row">
          <div class="small-5 column">
            <%= f.label :password_confirmation, class: "right inline" %>
          </div>
          <div class="small-7 column">
            <%= f.password_field :password_confirmation, autocomplete: "off",  id: "right-label" %>
          </div>
        </div>
        <div class="row">
          <div class="small-6 small-centered columns">
            <%= f.submit "Sign up", class: "button primary round expand" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>




